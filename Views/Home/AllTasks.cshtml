@{
}
@model List<MaitenanceTask>

<h1>All Tasks</h1>

@foreach (MaitenanceTask t in Model)
    {
<div class="card-body">
    <p>@t.Title</p>
    <p>Description: @t.Notes</p>
    <a class="btn btn-primary" href="@Url.Action("MaitenanceDetails", "Home", new {taskId = t.MaitenanceId})">Maitenance Details</a>
</div>
    }